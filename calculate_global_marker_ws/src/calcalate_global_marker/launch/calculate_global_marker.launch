<launch>
<node name="baselink_to_cameralink" pkg="tf" type="static_transform_publisher" args="0.18 0 0 0 0 0 base_link camera_link 50">
</node>

<node name="cameralink_to_lcameralink" pkg="tf" type="static_transform_publisher" args="0 0.06 0 -1.5707963 0 -1.5707963 camera_link mynteye_left_frame 50">
</node>

	<node name="calcalate_global_marker_node" pkg="calcalate_global_marker" type="calcalate_global_marker_node" respawn="false" output="screen">
	</node>

	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find calcalate_global_marker)/rviz/axis.rviz">
	</node>
</launch>
