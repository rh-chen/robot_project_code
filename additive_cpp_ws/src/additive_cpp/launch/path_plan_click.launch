<launch>
	<arg name="map_file" default="$(find additive_cpp)/maps/bad_poselist.yaml"/>

    <node name="map_server_for_test" pkg="map_server" type="map_server" args="$(arg map_file)">
    </node>

    <node name="additive_cpp_node" pkg="additive_cpp" type="additive_cpp_node" output="screen">
    </node>

	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find additive_cpp)/rviz/path_plan.rviz" />

</launch>
